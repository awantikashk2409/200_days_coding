   "source": [
    "def rotateleft(lst,k):\n",
    "    n = len(lst)\n",
    "    if n==0:\n",
    "        return lst\n",
    "    else:\n",
    "        k=k%n\n",
    "        return lst[k: ]+lst[ :k]"
   ]
